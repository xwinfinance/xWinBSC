    function transferFrom(address from, address to, uint value) external returns (bool);

    function DOMAIN_SEPARATOR() external view returns (bytes32);
    function PERMIT_TYPEHASH() external pure returns (bytes32);
    function nonces(address owner) external view returns (uint);

    function permit(address owner, address spender, uint value, uint deadline, uint8 v, bytes32 r, bytes32 s) external;

    event Mint(address indexed sender, uint amount0, uint amount1);
    event Burn(address indexed sender, uint amount0, uint amount1, address indexed to);
    event Swap(
        address indexed sender,
        uint amount0In,
        uint amount1In,
        uint amount0Out,
        uint amount1Out,
        address indexed to
    );
    event Sync(uint112 reserve0, uint112 reserve1);

    function MINIMUM_LIQUIDITY() external pure returns (uint);
    function factory() external view returns (address);
    function token0() external view returns (address);
    function token1() external view returns (address);
    function getReserves() external view returns (uint112 reserve0, uint112 reserve1, uint32 blockTimestampLast);
    function price0CumulativeLast() external view returns (uint);
    function price1CumulativeLast() external view returns (uint);
    function kLast() external view returns (uint);

    function mint(address to) external returns (uint liquidity);
    function burn(address to) external returns (uint amount0, uint amount1);
    function swap(uint amount0Out, uint amount1Out, address to, bytes calldata data) external;
    function skim(address to) external;
    function sync() external;

    function initialize(address, address) external;
}

library PancakeLibrary {
    using SafeMath for uint;

    // returns sorted token addresses, used to handle return values from pairs sorted in this order
    function sortTokens(address tokenA, address tokenB) internal pure returns (address token0, address token1) {
        require(tokenA != tokenB, 'PancakeLibrary: IDENTICAL_ADDRESSES');
        (token0, token1) = tokenA < tokenB ? (tokenA, tokenB) : (tokenB, tokenA);
        require(token0 != address(0), 'PancakeLibrary: ZERO_ADDRESS');
    }

    // calculates the CREATE2 address for a pair without making any external calls
    function pairFor(address factory, address tokenA, address tokenB) internal pure returns (address pair) {
        (address token0, address token1) = sortTokens(tokenA, tokenB);
        pair = address(uint(keccak256(abi.encodePacked(
                hex'ff',
                factory,
                keccak256(abi.encodePacked(token0, token1)),
                hex'00fb7f630766e6a796048ea87d01acd3068e8ff67d078148a3fa3f4a84f69bd5' // init code hash
            ))));
    }

    // fetches and sorts the reserves for a pair
    function getReserves(address factory, address tokenA, address tokenB) internal view returns (uint reserveA, uint reserveB) {
        (address token0,) = sortTokens(tokenA, tokenB);
        pairFor(factory, tokenA, tokenB);
        (uint reserve0, uint reserve1,) = IPancakePair(pairFor(factory, tokenA, tokenB)).getReserves();
        (reserveA, reserveB) = tokenA == token0 ? (reserve0, reserve1) : (reserve1, reserve0);
    }

    // given some amount of an asset and pair reserves, returns an equivalent amount of the other asset
    function quote(uint amountA, uint reserveA, uint reserveB) internal pure returns (uint amountB) {
        require(amountA > 0, 'PancakeLibrary: INSUFFICIENT_AMOUNT');
        require(reserveA > 0 && reserveB > 0, 'PancakeLibrary: INSUFFICIENT_LIQUIDITY');
        amountB = amountA.mul(reserveB) / reserveA;
    }

    // given an input amount of an asset and pair reserves, returns the maximum output amount of the other asset
    function getAmountOut(uint amountIn, uint reserveIn, uint reserveOut) internal pure returns (uint amountOut) {
        require(amountIn > 0, 'PancakeLibrary: INSUFFICIENT_INPUT_AMOUNT');
        require(reserveIn > 0 && reserveOut > 0, 'PancakeLibrary: INSUFFICIENT_LIQUIDITY');
        uint amountInWithFee = amountIn.mul(9975);
        uint numerator = amountInWithFee.mul(reserveOut);
        uint denominator = reserveIn.mul(10000).add(amountInWithFee);
        amountOut = numerator / denominator;
    }

    // given an output amount of an asset and pair reserves, returns a required input amount of the other asset
    function getAmountIn(uint amountOut, uint reserveIn, uint reserveOut) internal pure returns (uint amountIn) {
        require(amountOut > 0, 'PancakeLibrary: INSUFFICIENT_OUTPUT_AMOUNT');
        require(reserveIn > 0 && reserveOut > 0, 'PancakeLibrary: INSUFFICIENT_LIQUIDITY');
        uint numerator = reserveIn.mul(amountOut).mul(10000);
        uint denominator = reserveOut.sub(amountOut).mul(9975);
        amountIn = (numerator / denominator).add(1);
    }

    // performs chained getAmountOut calculations on any number of pairs
    function getAmountsOut(address factory, uint amountIn, address[] memory path) internal view returns (uint[] memory amounts) {
        require(path.length >= 2, 'PancakeLibrary: INVALID_PATH');
        amounts = new uint[](path.length);
        amounts[0] = amountIn;
        for (uint i; i < path.length - 1; i++) {
            (uint reserveIn, uint reserveOut) = getReserves(factory, path[i], path[i + 1]);
            amounts[i + 1] = getAmountOut(amounts[i], reserveIn, reserveOut);
        }
    }

    // performs chained getAmountIn calculations on any number of pairs
    function getAmountsIn(address factory, uint amountOut, address[] memory path) internal view returns (uint[] memory amounts) {
        require(path.length >= 2, 'PancakeLibrary: INVALID_PATH');
        amounts = new uint[](path.length);
        amounts[amounts.length - 1] = amountOut;
        for (uint i = path.length - 1; i > 0; i--) {
            (uint reserveIn, uint reserveOut) = getReserves(factory, path[i - 1], path[i]);
            amounts[i - 1] = getAmountIn(amounts[i], reserveIn, reserveOut);
        }
    }
}


contract xWinSale is Ownable {
    
    struct UserInfo {
        uint buyAmount;     
        uint releaseDay; 
        uint accBonus; 
        uint nextBuyBlock; 
    }
    
    using SafeMath for uint256;

    string public name;
    address public targetToken;
    address private platformaddress;
    IPancakeRouter02 pancakeSwapRouter;
    xWinMaster private _xWinMaster;
    mapping(address => UserInfo) public userInfoXWIN;
    mapping(address => UserInfo) public userInfoLP;
    uint public accXWINBonusPercent = 1000; 
    uint public accLPBonusPercent = 1000; 
    uint public lockperiod = 201600; // 7 days
    uint public lockperiodNextBuy = 28800; // 1 days
    uint public maxBuy = 5; // 5 BNB max per buy
    
    address public saleBonusAddress;
    bool public pauseXWIN = false;
    bool public pauseLP = false;
    
    event Received(address, uint);

    constructor(
        string memory _name,
        address _targetToken,
        address _platformaddress,
        address _masterOwner
        ) public {
        
        platformaddress = _platformaddress;
        targetToken = _targetToken;
        _xWinMaster = xWinMaster(_masterOwner);
        name = _name;
    }
    
    receive() external payable {
        emit Received(msg.sender, msg.value);
    }
    
    /// @dev owner to pause for depositing
    function setPause(bool _pauseXWIN, bool _pauseLP) public onlyOwner {
        pauseLP = _pauseLP;
        pauseXWIN = _pauseXWIN;
    }
    
    //only owner can update platform address
    function updatePlatformWallet(address _platformaddress) public onlyOwner {
        platformaddress = _platformaddress;
    }
    
    //only owner can update this as part of the setup 
    function updateProperties(uint _lockperiod, uint _lockperiodNextBuy, uint _accLPBonusPercent, uint _accXWINBonusPercent, uint _maxBuy) public onlyOwner {
        accLPBonusPercent = _accLPBonusPercent;
        accXWINBonusPercent = _accXWINBonusPercent;
        lockperiod = _lockperiod;
        lockperiodNextBuy = _lockperiodNextBuy;
        maxBuy = _maxBuy;
    }
    
    //only owner can update this as part of the setup 
    function updateSaleBonusContract(address _salebonusaddress) public onlyOwner {
        saleBonusAddress = _salebonusaddress;
    }
    
    function getBNBToXWIN(uint amount) public view returns (uint){
            
        IPancakeRouter02 router = _xWinMaster.getPancakeRouter();
        address[] memory path = new address[](2);
        path[0] = router.WETH();
        path[1] = targetToken;
        
        uint256[] memory amounts = router.getAmountsOut(amount, path);
        uint256 amountOut = amounts[amounts.length.sub(1)];
        return amountOut;
    }
    
    function _getQuotesForToken(
        uint256 _amount
    ) internal view 
        returns (uint amountB) {
        
        IPancakeRouter02 router = _xWinMaster.getPancakeRouter();
        (uint reserveA, uint reserveB) = PancakeLibrary.getReserves(router.factory(), router.WETH(), targetToken);
        amountB = PancakeLibrary.quote(_amount, reserveA, reserveB);
        return amountB;
    }
    
    function getXWINToBNB(uint amount) public view returns (uint){
            
        IPancakeRouter02 router = _xWinMaster.getPancakeRouter();
        address[] memory path = new address[](2);
        path[0] = targetToken;
        path[1] = router.WETH();
        
        uint256[] memory amounts = router.getAmountsOut(amount, path);
        uint256 amountOut = amounts[amounts.length.sub(1)];
        return amountOut;
    }
    
    function _swapTokenToBNB(uint amountIn, address destAddress) internal {
            
        IPancakeRouter02 router = _xWinMaster.getPancakeRouter();
        address[] memory path = new address[](2);
        path[0] = targetToken;
        path[1] = router.WETH();
        
        TransferHelper.safeApprove(targetToken, address(router), amountIn); 
        
        uint256[] memory amounts = router.getAmountsOut(amountIn, path);
        uint256 amountOut = amounts[amounts.length.sub(1)];
        
        router.swapExactTokensForETH(amountIn, amountOut.sub(amountOut.mul(100).div(10000)), path, destAddress, block.timestamp);
            
    }
    
    /// @dev return user infor for buying XWIN token
    function getUserInfoForXWIN(address _fromAddress) external view returns (uint, uint, uint){
                
         UserInfo memory _user =  userInfoXWIN[_fromAddress];
         return (_user.releaseDay, _user.accBonus, _user.buyAmount);
    }
    
    /// @dev return user infor for buying LP token
    function getUserInfoForLP(address _fromAddress) external view returns (uint, uint, uint){
                
         UserInfo memory _user =  userInfoLP[_fromAddress];
         return (_user.releaseDay, _user.accBonus, _user.buyAmount);
    }
    
    /// @dev only authorized contract can reset user info
    function resetUserXWINInfo(address _fromAddress) external{
        
        require(saleBonusAddress == msg.sender, "not authorized to reset");
        UserInfo storage _user =  userInfoXWIN[_fromAddress];
        _user.buyAmount = 0;
        _user.releaseDay = 0;
        _user.accBonus = 0;
    }
    
    /// @dev only authorized contract can reset user info
    function resetUserLPInfo(address _fromAddress) external{
        
        require(saleBonusAddress == msg.sender, "not authorized to reset");
        UserInfo storage _user =  userInfoLP[_fromAddress];
        _user.buyAmount = 0;
        _user.releaseDay = 0;
        _user.accBonus = 0;
    }
    
    /// @dev user to buy XWIN token
    function buyToken() external payable {
        
        require(pauseXWIN == false, "pause for buying");
        require(msg.value <= maxBuy.mul(1e18), "more than max buy");
        uint xWINBalance = IBEP20(targetToken).balanceOf(address(this));
        require(xWINBalance > 0, "no more subs");
        require(msg.value > 0, "no BNB sent");

        UserInfo storage _user =  userInfoXWIN[msg.sender];
        require(_user.nextBuyBlock < block.number, "wait for next buying chance");

        uint entitledXWIN = getBNBToXWIN(msg.value);
        require(xWINBalance >= entitledXWIN, "not enough token for this round");
        
        uint bonus =  entitledXWIN.mul(accXWINBonusPercent).div(10000);
        TransferHelper.safeTransfer(targetToken, saleBonusAddress, bonus.add(entitledXWIN)); //send bonus and half to bonus for later claim
        
        uint tokenAmt = _getQuotesForToken(msg.value);
        _stakeTokens(tokenAmt, msg.value, platformaddress); //stake proceeds to LP

        //update user stats
        _user.releaseDay = block.number.add(lockperiod);
        _user.buyAmount = _user.buyAmount.add(entitledXWIN);
        _user.accBonus = _user.accBonus.add(bonus);
        _user.nextBuyBlock = block.number.add(lockperiodNextBuy);
    }
    
    /// @dev user to sell XWIN token in pancakeswap
    function sellToken(uint amount) external payable {
        
        require(IBEP20(targetToken).balanceOf(msg.sender) >= amount, "Not enough token");
        TransferHelper.safeTransferFrom(targetToken, msg.sender, address(this), amount);
        _swapTokenToBNB(amount, msg.sender);
    }
    
    /// @dev user to buy locked XWIN token
    function buyLPToken() external payable {
        
        require(pauseLP == false, "pause for buying");
        require(msg.value <= maxBuy.mul(1e18).mul(2), "more than max buy");
        UserInfo memory _user =  userInfoLP[msg.sender];
        require(_user.nextBuyBlock < block.number, "wait for next buying chance");
        _buyLPToken(msg.sender, msg.value);
    }
    
    
    function _buyLPToken(address _from, uint amountBNB) internal  {
        
        uint xWINBalance = IBEP20(targetToken).balanceOf(address(this));
        require(xWINBalance > 0, "no more subs");
        require(amountBNB > 0, "no BNB sent");
        
        uint half = amountBNB.div(2);
        uint tokenAmt = _getQuotesForToken(half);
        
        uint bonus =  tokenAmt.mul(accLPBonusPercent).div(10000);
        require(xWINBalance >= tokenAmt.add(tokenAmt).add(bonus), "not enough token");
        
        //stake for users
        uint liquidity = _stakeTokens(tokenAmt, half, saleBonusAddress);
        //stake proceeds to LP
        uint tokenAmtForPlatform = _getQuotesForToken(address(this).balance);
        _stakeTokens(tokenAmtForPlatform, address(this).balance, platformaddress); 

        UserInfo storage _user =  userInfoLP[_from];
        _user.releaseDay = block.number.add(lockperiod);
        _user.buyAmount = _user.buyAmount.add(liquidity);
        _user.accBonus = _user.accBonus.add(bonus);
        _user.nextBuyBlock = block.number.add(lockperiodNextBuy);
        
        //transfer necessary xwin token to bonus pool
        TransferHelper.safeTransfer(targetToken, saleBonusAddress, bonus);
    }
    
    function _stakeTokens(uint256 tokenQty, uint256 bnbQty, address _toAddress) internal
        returns (uint){
        
        (, , uint liquidity) = _addLiquidityBNB(tokenQty, bnbQty, _toAddress);
        return liquidity;
    }
    
    function _addLiquidityBNB(uint tokenQty, uint bnbAmt, address _toAddress) internal 
        returns (uint amountToken, uint amountBNB, uint liquidity) {
            
        IPancakeRouter02 router = _xWinMaster.getPancakeRouter();
        TransferHelper.safeApprove(targetToken, address(router), tokenQty); 
        (amountToken, amountBNB, liquidity) = router.addLiquidityETH{value: bnbAmt}(
            targetToken,
            tokenQty,
            tokenQty.mul(9950).div(10000),
            bnbAmt.mul(9950).div(10000), 
            _toAddress,
            block.timestamp
            );
        return (amountToken, amountBNB, liquidity);
    }
    
    /// @dev trf unsold XWN token back to xwin team
    function transferAfterEnd(address _to, uint balance) public onlyOwner {
        uint256 xwinTokenBal = IBEP20(targetToken).balanceOf(address(this));
        require(xwinTokenBal >= balance, "not enough to transfer");
        TransferHelper.safeTransfer(targetToken, _to, balance);
    }
}